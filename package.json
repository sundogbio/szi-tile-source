{
  "name": "szi-tile-source",
  "version": "0.5.1",
  "description": "An OpenSeadragon TileSource for remotely hosted SZI files",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/szi-tile-source.umd.cjs",
  "module": "./dist/szi-tile-source.js",
  "exports": {
    ".": {
      "import": "./dist/szi-tile-source.js",
      "require": "./dist/szi-tile-source.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build_site": "vite build --config pages.config.js",
    "preview": "vite preview",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sundogbio/szi-tile-source.git"
  },
  "private": true,
  "devDependencies": {
    "fast-xml-parser": "^5.2.5",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "openseadragon": "^5.0.1",
    "prettier": "^3.6.2",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "openseadragon": "^5.0.0"
  }
}
